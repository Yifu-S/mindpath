{
  "name": "mindpath",
  "version": "1.0.0",
  "description": "Privacy-first mental health support platform for college students",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "setup": "node setup.js",
    "build": "npm run setup && npm install",
    "docker:build": "docker build -t mindpath .",
    "docker:run": "docker run -p 3000:3000 --env-file .env mindpath",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop mindpath",
    "pm2:restart": "pm2 restart mindpath",
    "migrate": "node migrations/run.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.6",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "openai": "^4.24.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "mental-health",
    "privacy",
    "encryption",
    "college-students",
    "ai-support"
  ],
  "author": "MindPath Team",
  "license": "MIT"
}